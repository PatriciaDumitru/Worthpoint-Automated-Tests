        <groups>
            <run>
                <include name="eComm_Orders"/>
                <exclude name="Unstable"/>
            </run>
        </groups> 
        <classes>
            <class name="com.coats.selenium.tests.Ecomm_SUSST_ME_IT"/>
            <class name="com.coats.selenium.tests.Ecomm_SUSST_UORT_IT"/>
            <class name="com.coats.selenium.tests.Ecomm_SUSST_FEBO_IT"/>
