<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="QuickSuite">

    <!-- see examples at http://testng.org/doc/documentation-main.html#testng-xml -->
    
    <test name="QuickTest">
        <groups>
            <run>
                <include name="QuickTest"/>
                <exclude name="Unstable"/>
            </run>
        </groups>
        <packages>
            <package name="com.coats.selenium.tests"/>
            <package name="com.coats.selenium.RCTests"/>
        </packages>
    </test>

</suite>
